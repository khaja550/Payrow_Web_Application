{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "payrow-web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/payrow-web",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/scss/bootstrap.scss",
			  "node_modules/ngx-toastr/toastr.css",
              "node_modules/@ng-select/ng-select/themes/material.theme.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "src/assets/js/jquery-ui.min.js",
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/js/Chart.min.js",
              "src/assets/js/sparkline.js",
              "src/assets/js/jquery.knob.min.js",
              "src/assets/js/moment.min.js",
              "src/assets/js/daterangepicker.js",
              "src/assets/js/tempusdominus-bootstrap-4.min.js",
              "src/assets/js/summernote-bs4.min.js",
              "src/assets/js/jquery.overlayScrollbars.min.js",
              "src/assets/js/adminlte.js",
              "src/assets/js/fusioncharts.js",
              "src/assets/js/jquery.simple-bar-graph.min.js",
              "src/assets/js/jquery.dataTables.min.js",
              "src/assets/js/main.js",
              "src/assets/js/dashboard.js"
            ],
            "allowedCommonJsDependencies": [
              "lodash"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "payrow-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "payrow-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "payrow-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "payrow-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "payrow-web:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "payrow-web",
  "cli": {
    "analytics": "036228a7-dee4-492f-9ce6-03a81f77494a"
  }
}